{
  "author": {
    "name": "Jon Schlinkert",
    "url": "https://github.com/jonschlinkert"
  },
  "dependencies": {
    "ansi-cyan": "^0.1.1",
    "ansi-dim": "^0.1.1",
    "prompt-radio": "^1.2.1"
  },
  "deprecated": false,
  "description": "List-style prompt. Can be used as a standalone prompt, or with a prompt system like [enquirer].",
  "devDependencies": {
    "choices-separator": "^2.0.0",
    "enquirer": "^1.0.3",
    "gulp-format-md": "^1.0.0",
    "mocha": "^5.2.0"
  },
  "engines": {
    "node": ">=4"
  },
  "files": [
    "index.js"
  ],
  "homepage": "https://github.com/enquirer/prompt-list",
  "license": "MIT",
  "main": "index.js",
  "name": "prompt-list",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enquirer/prompt-list.git"
  },
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "data": {
      "alias": "list"
    },
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "enquirer",
        "enquirer-prompt",
        "enquirer-question"
      ]
    },
    "lint": {
      "reflinks": true
    }
  },
  "version": "3.2.0"
}