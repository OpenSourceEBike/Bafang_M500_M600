            sortArr (arr, str) {
                var _arr = [], _t = [];
                var _temp;
                // 对数组的元素进行排序，并返回数组。
                arr = arr.sort(function (a, b) {
                    var s = a[str],
                    t = b[str]
                    return s < t ? -1 : 1;
                })
                if (arr.length) {
                    _temp = arr[0][str]
                }
                // 将相同类别的对象添加到统一个数组
                for (var i in arr) {
                    if (arr[i][str] === _temp) {
                        _t.push(arr[i]);
                    } else {
                        _temp = arr[i][str];
                        _arr.push(_t);
                        _t = [arr[i]];
                    }
                }
                // 将最后的内容推出
                _arr.push(_t)
                return _arr
            },
